@model Week11aCRUDDataFirst.Models.Employee
@{
    ViewData["Title"] = "Update";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Update</h1>

<form asp-controller="Home" asp-action="Update">

    <label>First Name</label>
    <input type="text" asp-for="FirstName" />
